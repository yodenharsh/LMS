{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleDetection": "force",
    "outDir": "./dist",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,

    "types": ["bun-types"]
  }
}
